{
  "name": "react-popper-tooltip",
  "version": "2.7.2",
  "description": "React tooltip library built around react-popper",
  "homepage": "https://mohsinulhaq.github.io/react-popper-tooltip/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohsinulhaq/react-popper-tooltip"
  },
  "browser": "lib/cjs/index.js",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "unpkg": "dist/index.js",
  "style": "dist/styles.css",
  "typings": "typings/react-popper-tooltip.d.ts",
  "files": [
    "lib",
    "dist",
    "typings/react-popper-tooltip.d.ts"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "rimraf dist lib && rollup -c && cp src/styles.css dist",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "docs": "docz dev",
    "docs:build": "docz build",
    "docs:deploy": "yarn docs:build && cp .docz/dist/index.html .docz/dist/404.html && gh-pages -d .docz/dist",
    "lint": "eslint .",
    "test": "jest"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "react",
    "tooltip",
    "popover",
    "overlay",
    "react-tooltip",
    "react-popper"
  ],
  "author": "Mohsin Ul Haq <mohsinulhaq01@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "react-popper": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "docz": "^0.12.16",
    "docz-plugin-css": "^0.11.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-testing-library": "^5.3.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-terser": "^3.0.0"
  }
}
