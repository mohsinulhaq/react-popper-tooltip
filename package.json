{
  "name": "react-popper-tooltip",
  "version": "2.8.2",
  "description": "React tooltip library built around react-popper",
  "homepage": "https://mohsinulhaq.github.io/react-popper-tooltip/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohsinulhaq/react-popper-tooltip"
  },
  "main": "dist/cjs/react-popper-tooltip.js",
  "module": "dist/esm/react-popper-tooltip.js",
  "style": "dist/styles.css",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c && cp src/styles.css dist && yarn tsc && rm -rf compiled",
    "prepublishOnly": "yarn typecheck && yarn lint && yarn test && yarn build",
    "docs": "docz dev",
    "docs:build": "docz build",
    "docs:deploy": "yarn docs:build && cp .docz/dist/index.html .docz/dist/404.html && gh-pages -d .docz/dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test": "jest"
  },
  "pre-commit": [
    "typecheck",
    "lint",
    "test"
  ],
  "keywords": [
    "react",
    "tooltip",
    "popover",
    "overlay",
    "react-tooltip",
    "react-popper"
  ],
  "author": "Mohsin Ul Haq <mohsinulhaq01@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "react-popper": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-testing-library": "^6.1.2",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "typescript": "^3.4.3"
  }
}
