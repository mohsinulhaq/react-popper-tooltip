{
  "name": "react-popper-tooltip",
  "version": "2.8.0",
  "description": "React tooltip library built around react-popper",
  "homepage": "https://mohsinulhaq.github.io/react-popper-tooltip/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohsinulhaq/react-popper-tooltip"
  },
  "main": "dist/cjs/react-popper-tooltip.js",
  "module": "dist/esm/react-popper-tooltip.js",
  "style": "dist/styles.css",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c && cp src/styles.css dist && yarn tsc && rimraf compiled",
    "prepublishOnly": "yarn typecheck && yarn lint && yarn test && yarn build",
    "docs": "docz dev",
    "docs:build": "docz build",
    "docs:deploy": "yarn docs:build && cp .docz/dist/index.html .docz/dist/404.html && gh-pages -d .docz/dist",
    "typecheck": "tsc --noEmit",
    "lint": "tslint -p .",
    "test": "jest"
  },
  "pre-commit": [
    "typecheck",
    "lint",
    "test"
  ],
  "keywords": [
    "react",
    "tooltip",
    "popover",
    "overlay",
    "react-tooltip",
    "react-popper"
  ],
  "author": "Mohsin Ul Haq <mohsinulhaq01@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "react-popper": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^5.4.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
